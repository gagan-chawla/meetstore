{
    "name": "Meet Store",
    "version": "1.1.0",
    "description": "Google meets to Google sheets",
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "manifest_version": 2
}
